                        return false;
                    } else {
                        $("#"+i+"_name").val(obj["file_info"]["file_name"]);
                        $("#"+i+"_size").val(obj["file_info"]["file_size"]);
                        $("#"+i+"_url").val(obj["file_info"]["file_url"]);

                        $("#"+i+"_ori_name").val(file_name);
